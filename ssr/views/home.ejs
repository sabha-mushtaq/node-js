<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student List</title>
</head>
<body>
    <h1>List of Students</h1>
    <ul>
        <% names.forEach(function(student) { %>
            <li>
                Name: <%= student.name %>, 
                Standard: <%= student.standard %>, 
                Marks: <%= student.marks %>, 
                Roll No: <%= student.rollno %>
            </li>
        <% }); %>
    </ul>
</body>
</html>
